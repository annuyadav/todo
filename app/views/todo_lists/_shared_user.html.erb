<div class="users-listing">
  <div class="col-sm-4 col-md-4">
    <div class="thumbnail">
      <div class="caption">
        <h4><%= user.full_name %></h4>

        <p><%= user.email %></p>
      </div>
      <div class="box">
        <% if todo_list.owner == current_user %>
            <%= link_to destroy_share_todo_list_path(todo_list, user), method: :delete, class: 'pull-right', data: {confirm: 'Are you sure?'}, remote: true do %>
                <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
            <% end %>
        <% end %>
      </div>
    </div>
  </div>
</div>
